{
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "module": "commonjs",
    "target": "es5",
    "allowJs": false,
    "noUnusedLocals": false,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "noImplicitThis": true,
    "baseUrl": ".",
    "paths": {
      "@odinlin/formily-antdv": ["packages/antdv/src"]
    },
    "lib": ["ESNext", "DOM"],
    "types": ["vue"]
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "include": [
    "./**/*.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.vue"
  ]
}
